  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LogSnag/logsnag.js/issues"
  },
  "homepage": "https://github.com/LogSnag/logsnag.js#readme",
  "dependencies": {
    "node-fetch": "^2.6.6"
  },
  "devDependencies": {
    "typescript": "^4.2.3",
    "@types/node": "^17.0.8",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "prettier": "^2.5.1"
  }
