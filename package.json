{
  "name": "logsnag",
  "version": "0.0.5",
  "description": "LogSnag Client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
